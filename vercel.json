{
    "buildCommand": "turbo run build --filter=@shop-wise/web",
    "outputDirectory": "dist",
    "framework": null,
    "functions": {
        "apps/api/api/index.js": {
            "runtime": "nodejs18.x"
        }
    },
    "env": {
        "SERVERLESS": "true"
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ]
}
